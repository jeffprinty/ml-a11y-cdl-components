{
  "name": "ml-a11y-cdl-components",
  "version": "1.0.0",
  "description": "Accessible react components that comply with the Macmillan Learning CDL",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlazul/ml-a11y-cdl-components.git"
  },
  "author": "Macmillan Learning",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mlazul/ml-a11y-cdl-components/issues"
  },
  "homepage": "https://github.com/mlazul/ml-a11y-cdl-components#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "jest": "^19.0.2",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "styled-components": "^1.4.5"
  }
}
